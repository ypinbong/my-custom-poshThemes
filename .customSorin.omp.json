{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#CB4B16",
          "style": "plain",
          "type": "exit"
        },
        {
          "foreground": "#CECE04",
          "style": "plain",
          "type": "root"
        },
        {
          "foreground": "#FFFFFF",
          "style": "plain",
          "type": "session"
        },
        {
          "foreground": "#0973C0",
          "properties": {
            "prefix": "",
            "style": "full"
          },
          "style": "plain",
          "type": "path"
        },
        {
          "foreground": "#C1C106",
          "properties": {
            "prefix": "<#ffffff>git:</>"
          },
          "style": "plain",
          "type": "git"
        },
        {
          "foreground": "#100e23",
          "properties": {
            "prefix": "  "
          },
          "style": "plain",
          "type": "python"
        },
        {
          "foreground": "#ffffff",
          "properties": {
            "prefix": "",
            "text": "<#CB4B16>❯</><#CACA02>❯</><#4E9A06>❯</>"
          },
          "style": "plain",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ]
}
