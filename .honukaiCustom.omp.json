{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#8B949E",
          "properties": {
            "text": ""
          },
          "style": "plain",
          "type": "text"
        },
        {
          "foreground": "#8B949E",
          "properties": {
            "prefix": "[",
            "postfix": "]"
          },
          "style": "plain",
          "type": "time"
        },
        {
          "foreground": "#5D5E60",
          "properties": {
            "folder_icon": "  ",
            "folder_separator_icon": "/",
            "postfix": "<#8B949E>  </>",
            "style": "full"
          },
          "style": "diamond",
          "type": "path"
        },
        {
          "foreground": "#0CAA70",
          "properties": {
            "prefix": "<#8B949E>on</>",
            "branch_icon": "<#0CAA70> </>",
            "postfix": "<#0CAA70> </>"
          },
          "style": "diamond",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#CECE04",
          "properties": {
            "prefix": ""
          },
          "style": "plain",
          "type": "root"
        },
        {
          "foreground": "#100e23",
          "properties": {
            "prefix": "  "
          },
          "style": "plain",
          "type": "python"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#fafafa",
          "leading_diamond": " ▓",
          "properties": {
            "postfix": "",
            "prefix": "",
            "text": ""
          },
          "style": "diamond",
          "type": "text"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#8B949E",
          "properties": {
            "postfix": "",
            "text": "  ",
            "text": "  ",
            "prefix": ""
          },
          "style": "diamond",
          "type": "text"
        },
        {
          "background": "#2f2f2f",
          "foreground": "#fafafa",
          "properties": {
            "postfix": "",
            "prefix": "",
            "text": ""
          },
          "style": "diamond",
          "trailing_diamond": "",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{if .Root}}root :: {{end}}{{.Shell}} :: {{.Folder}}",
  "final_space": true
}